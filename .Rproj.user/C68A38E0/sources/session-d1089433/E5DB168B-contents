# quick thing to have a look at ratio over time 
genVals <- function(start1, start2, iters = NULL, unitGain = 1){
  if(is.null(iters)){
    iters = max(start1, start2) * 3
  }
  x <- seq(start1, by = unitGain, length.out = iters)
  y <- seq(start2, by = unitGain, length.out = iters)
  
  plot(x, x/y)
  
}
genVals(13, 56, 100)
